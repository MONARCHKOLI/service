<%= form_with model: @user do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :mobileno %><br>
    <%= f.text_field :mobileno %><br>
    <%= f.label :address %><br>
    <%= f.text_area :address %><br>
    <%= f.label :city %><br>
    <%= f.text_field :city %><br>
    <%= f.label :pincode %><br>
    <%= f.text_field :pincode %><br>
    <%= f.label :state %><br>
    <%= f.text_field :state %><br>
    
    <%= f.label :email %><br>
    <%= f.text_field :email %><br>
    <%= f.label :new_password %><br>
    <%= f.password_field :password %><br>
    <%= f.submit "Submit!!"%>
<% end %>