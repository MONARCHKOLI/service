<%= form_with model: @worker do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :mobileno %><br>
    <%= f.text_field :mobileno %><br>
    <%= f.label :address %><br>
    <%= f.text_area :address %><br>



    
    <%= f.label :email %><br>
    <%= f.text_field :email %><br>
    <%= f.label :age %><br>
    <%= f.text_field :age %><br>
    
    <%= f.submit "Submit!!"%>
<% end %>